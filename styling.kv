
<MyTheme>
	on_release: root.set_theme_dialog_icon(check, root.text)
	CheckboxLeftWidget: 
		id: check 
		group: "check"
		theme_text_color: 'Custom'
		text_color: app.theme_cls.primary_color
		

<SecurityAgencyList>:
	theme_text_color: "Custom" 
	text_color: app.theme_cls.opposite_bg_dark
	on_release: app.show_agency_dialog(root.text, root.secondary_text, root.tertiary_text)
	secondary_font_style : "Caption"

	IconLeftWidget:
		id: icon
		icon: root.icon
		theme_text_color: 'Custom'
		text_color: app.theme_cls.accent_color
							
<SettingsList>:
	theme_text_color: "Custom" 
	text_color: app.theme_cls.opposite_bg_dark
	on_release: app.set_settings(root.text)
	secondary_font_style : "Caption"

	IconLeftWidget:
		id: icon
		icon: root.icon
		theme_text_color: 'Custom'
		text_color: app.theme_cls.accent_color
		
<ReportsListItem>:
	text: root.reporterName
	secondary_text: root.incidentLocation
	theme_text_color: "Custom" 
	text_color: app.theme_cls.opposite_bg_dark
	on_release: app.set_report_screen(report = root)
	secondary_font_style : "Caption"

	IconLeftWidget:
		id: icon
		icon: "account-child-circle"
		theme_text_color: 'Custom'
		text_color: app.theme_cls.accent_color
				

